                                                        %%[SET @noSMSRegions = Substring(@noSMSRegions, 1, Subtract(Length(@noSMSRegions), 2))]%%
                                                        %%[SET @noSMSRegions = Replace(@noSMSRegions, ", ", ", ")]%%
                                                        <p style="padding: 5px 10px 5px; font-size: 10pt; line-height: 1.3; font-family: Helvetica, Arial, sans-serif;"><b>No SMS sents for:</b> %%=v(@noSMSRegions)=%%</p>
